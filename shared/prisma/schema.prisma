// shared/prisma/schema.prisma
generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

// Example model â€” remove/modify for your real tables
model User {
  id    Int    @id @default(autoincrement())
  name  String
  email String @unique
}
